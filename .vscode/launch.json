		{
			"name": "Launch and Debug Hosted Blazor WebAssembly App",
			"type": "blazorwasm",
			"request": "launch",
			"hosted": true,
			"program": "${workspaceFolder}/bin/Debug/<target-framework>/<project-name.dll>",
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "DotNetWatch",
			"request": "launch",
			"name": ".NET Core Watch",
			"args": [],
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			}
		},
